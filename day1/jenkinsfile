pipeline {
  agent any

  stages {
    stage('Initialize') {
      steps {
        echo 'Initializing...'
        sh "pwd"
      }
    }

    stage('Build') {
      steps {
        echo 'Building...'
      }
    }

    stage('Test') {
      steps {
        echo 'Testing...'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Deploying...'
      }
    }
  }
}